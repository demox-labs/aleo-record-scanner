{
  "name": "rpc",
  "version": "1.0.0",
  "scripts": {
    "build:dev": "NODE_ENV=development webpack --mode development",
    "start:dev": "NODE_ENV=development nodemon",
    "build:prod": "NODE_ENV=production webpack --mode production",
    "start:prod": "NODE_ENV=production yarn migrate && NODE_ENV=production node --max-http-header-size=1024 --experimental-modules --max-old-space-size=14384 ./dist/index.js",
    "lint": "eslint ./src --ext .ts",
    "test:wasm": "yarn build:dev && NODE_ENV=development node --max-http-header-size=1024 --experimental-modules ./dist/wasmTest.js",
    "test:jest": "yarn migrate-test && jest --detectOpenHandles",
    "test": "yarn test:jest && yarn test:wasm",
    "test-watch": "yarn test-db-up && yarn migrate-test && jest --watch",
    "migrate": "ts-node ./node_modules/.bin/knex --esm --debug --knexfile ./src/db/knexfile.ts migrate:latest",
    "migrate-next": "ts-node ./node_modules/.bin/knex --esm --debug --knexfile ./src/db/knexfile.ts migrate:up",
    "migrate-test": "NODE_ENV=test ts-node ./node_modules/.bin/knex --esm --debug --knexfile ./src/db/knexfile.ts migrate:latest",
    "rollback": "ts-node ./node_modules/.bin/knex --esm --knexfile ./src/db/knexfile.ts migrate:down",
    "rollback-all": "ts-node ./node_modules/.bin/knex migrate:rollback --esm --knexfile ./src/db/knexfile.ts migrate:rollback --all",
    "rollback-test": "NODE_ENV=test ts-node ./node_modules/.bin/knex --esm --knexfile ./src/db/knexfile.ts migrate:down",
    "rollback-all-test": "NODE_ENV=test ts-node ./node_modules/.bin/knex --esm --knexfile ./src/db/knexfile.ts migrate:rollback --all",
    "create-migration": "ts-node ./node_modules/.bin/knex --esm --knexfile ./src/db/knexfile.ts migrate:make",
    "test-db-up": "docker compose --profile test up -d",
    "test-db-down": "docker stop unittest",
    "test-db-already-up": "yarn migrate-test && yarn test",
    "local-db-up": "docker compose --profile dev up --build -d",
    "local-db-down": "docker compose --profile dev down",
    "docs": "ts-node --project ./tsconfig-utility.json ./util/docs.ts",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "protobuf": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=. ./src/proto/records.proto"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn docs"
    }
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.293.0",
    "@demox-labs/aleo-sdk-staging": "^0.3.35",
    "@noble/curves": "^1.0.0",
    "@types/async-lock": "^1.4.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/bech32": "^1.1.4",
    "@types/bn.js": "^5.1.1",
    "@types/cors": "^2.8.13",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.15",
    "@types/node-fetch": "^2.6.2",
    "@types/pg": "^8.6.5",
    "@types/uuid": "^9.0.8",
    "async-lock": "^1.4.0",
    "aws-sdk": "^2.1464.0",
    "bech32": "^2.0.0",
    "bech32-buffer": "^0.2.1",
    "bn.js": "^5.2.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "ioredis": "^5.2.5",
    "json-rpc-2.0": "^1.4.1",
    "knex": "^2.4.2",
    "node-fetch": "^2.6.8",
    "pg": "^8.8.0",
    "queue-typescript": "^1.0.1",
    "threads": "^1.7.0",
    "ts-proto": "^1.166.3",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.9.4",
    "uuid": "^9.0.1",
    "winston": "^3.8.2",
    "winston-cloudwatch": "^6.1.1",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-typescript": "^7.23.3",
    "@types/jest": "^29.2.4",
    "@types/node": "^20.10.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^9.1.3",
    "babel-preset-env": "^1.7.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.28.0",
    "eslint-plugin-security": "^1.7.1",
    "find": "^0.3.0",
    "fs-extra": "^9.0.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-fetch-mock": "^3.0.3",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "wasm-loader": "^1.3.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
